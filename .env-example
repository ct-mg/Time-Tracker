# the key of the extension used in ChurchTools
VITE_KEY=extensionkey

# Build mode: "simple" (single bundle) or "advanced" (code splitting with lazy loading)
# Simple mode (default): All entry points in one bundle - best for small extensions
# Advanced mode: Automatic code splitting - best for large extensions with many entry points
VITE_BUILD_MODE=simple

# for local development provide the url and credentials
VITE_BASE_URL=https://xyz.church.tools
VITE_USERNAME=churchtools
VITE_PASSWORD=churchtools